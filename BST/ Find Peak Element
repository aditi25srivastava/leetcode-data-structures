class Solution(object):
    def findPeakElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        left, right = 0, len(nums) - 1
        while left < right:
            mid = (left + right) // 2

            # peak is on the left side
            if nums[mid] >nums[mid + 1]:
                  right = mid

            # right side
            else:
                left = mid + 1

        return left          
