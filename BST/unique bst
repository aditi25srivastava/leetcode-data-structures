class Solution(object):
    def numTrees(self, n):
        """
        :type n: int
        :rtype: int
        """
        dp = [0] * (n + 1)  # dp[i] will store the number of unique BSTs that can be made with i nodes
        dp[0] = 1 # One empty tree
         
        for nodes in range(1, n + 1):
            for root in range(1, nodes + 1):
                left = dp[root - 1] # Number of BSTs in left subtree
                right = dp[nodes - root] # Number of BSTs in right subtree
                dp[nodes] += left * right
        return dp[n]  
